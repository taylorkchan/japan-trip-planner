# Backend Mode Configuration
USE_SUPABASE=true

# Supabase Configuration (when USE_SUPABASE=true)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Custom Backend Configuration (when USE_SUPABASE=false)
DATABASE_URL=postgresql://username:password@localhost:5432/japan_trip_planner
JWT_SECRET=your_super_secret_jwt_key
JWT_EXPIRES_IN=7d

# AWS Configuration (Custom Backend)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET_NAME=japan-trip-planner-assets

# Redis Configuration (Custom Backend)
REDIS_URL=redis://localhost:6379

# Server Configuration
PORT=5000
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# External APIs
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
OPENWEATHER_API_KEY=your_openweather_api_key