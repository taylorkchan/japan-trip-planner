{
  "version": 2,
  "builds": [
    {
      "src": "Source Code/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "Source Code/backend/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Source Code/backend/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "Source Code/frontend/dist/$1"
    }
  ],
  "functions": {
    "Source Code/backend/src/index.ts": {
      "includeFiles": "Source Code/backend/prisma/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}